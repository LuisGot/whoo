{
  "name": "whoo",
  "version": "0.1.0",
  "description": "CLI to access your Whoop Health data through your terminal",
  "type": "module",
  "bin": {
    "whoo": "./src/index.ts"
  },
  "files": [
    "src",
    "README.md"
  ],
  "scripts": {
    "start": "bun run src/index.ts",
    "build": "mkdir -p dist && bun build --compile src/index.ts --outfile dist/whoo",
    "install:local": "mkdir -p ~/.local/bin && install -m 755 dist/whoo ~/.local/bin/whoo",
    "prepublishOnly": "bun run test && bun run typecheck",
    "test": "bun test",
    "typecheck": "bunx tsc --noEmit"
  },
  "devDependencies": {
    "@types/bun": "^1.3.0",
    "typescript": "^5.9.0"
  },
  "dependencies": {
    "@inquirer/prompts": "^8.2.1",
    "conf": "^15.1.0",
    "open": "^11.0.0"
  }
}
